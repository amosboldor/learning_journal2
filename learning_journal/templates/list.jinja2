{% extends "layout.jinja2" %}
{% block title %} Sera Shioda Smith - Learning Journal - Entries {% endblock %}

    <!-- Body Layout -->  

    {% block body %} 
        <body>
        {% for entry in entries %}
            <!--banner-starts-->
            <div class="banner1">
                <div class="container">
                    <div class="banner1-top">
                        <div class="banner1-text">
                            <h2>{{ entry.title }}</h2>
                            <h1>{{ entry.title1 }}</h1>
                            <h1>Created: {{ entry.create_date }}</h1>
                            <p>Entry: {{ entry.body }}</p>
                            <div class="about-btn">
                                <a href="{{ request.route_url('detail', id=entry.id) }}">Read More</a>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <!--banner-end-->
        {% endfor %}
        </body>

    {% endblock %}

    <!-- end Body Layout -->



